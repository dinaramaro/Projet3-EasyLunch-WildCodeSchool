        "jsx"
      ],
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/mouse-events-have-key-events": "off",
      "react/prop-types": "off",
      "no-shadow": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "husky": "^1.2.0"
